    <color name="colorPrimary">#0077B6</color>
    <color name="colorPrimaryDark">#005A8C</color>
    <color name="ic_launcher_background">#F0F7FA</color>
    <color name="splash_background">#0077B6</color>
    <string name="app_name">Zwemsterdam</string>
    <string name="custom_url_scheme">nl.zwemsterdam.app</string>
    <string name="package_name">nl.zwemsterdam.app</string>
    <string name="title_activity_main">Zwemsterdam</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>
